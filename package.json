{
  "name": "lol replay recorder",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --clean --dts --sourcemap",
    "lint": "eslint --fix .",
    "lint-check": "eslint .",
    "start": "cross-env NODE_ENV=production node --loader ts-node/esm ./src/index.ts",
    "test": "cross-env NODE_ENV=test mocha --timeout 5000 --exit -r ts-node/register 'test/**/*.ts'",
    "test:e2e": "cross-env NODE_ENV=test mocha --timeout 5000 --exit  -r ts-node/register 'test/e2e/*.ts'",
    "test:unit": "cross-env NODE_ENV=test mocha --timeout 5000 --exit --parallel -r ts-node/register 'test/unit/*.ts'"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/chai": "^4.3.13",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemon": "1.19.6",
    "@types/shelljs": "0.8.15",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "chai": "^5.1.0",
    "cross-env": "^7.0.3",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.3.0",
    "msw": "^2.2.9",
    "nodemon": "^3.1.0",
    "prettier": "3.2.5",
    "shelljs": "0.8.5",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.2",
    "typescript": "^5.4.2"
  },
  "packageManager": "yarn@3.6.0",
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.19",
    "@google-cloud/local-auth": "^3.0.1",
    "@types/bluebird": "^3.5.42",
    "@types/express": "^4.17.21",
    "axios": "^1.6.8",
    "bluebird": "^3.7.2",
    "bottleneck": "^2.19.5",
    "league-connect": "^6.0.0-rc12",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "node-fetch": "^3.3.2",
    "reflect-metadata": "^0.2.1",
    "undici": "^6.9.0"
  },
  "type": "module"
}
