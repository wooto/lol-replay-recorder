{
  "name": "lol replay recorder",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint --fix .",
    "lint-check": "eslint .",
    "start": "node --loader ts-node/esm ./src/index.ts",
    "test": "mocha --timeout 5000 --exit -r ts-node/register 'test/**/*.ts'",
    "test:e2e": "mocha --timeout 5000 --exit  -r ts-node/register 'test/e2e/*.ts'",
    "test:unit": "mocha --timeout 5000 --exit --parallel -r ts-node/register 'test/unit/*.ts'"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.2",
    "@types/chai": "^4.3.13",
    "@types/lodash": "^4.17.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.30",
    "@types/node-fetch": "^2.6.11",
    "@types/nodemon": "1.19.6",
    "@types/shelljs": "0.8.15",
    "@types/sinon": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "chai": "^5.1.0",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "mocha": "^10.3.0",
    "prettier": "3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "@fightmegg/riot-api": "^0.0.19",
    "league-connect": "^6.0.0-rc12"
  },
  "type": "module"
}
